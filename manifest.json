{
    "manifest_version": 3,
    "name": "UglyFrog",
    "version": "0.1",
    "description": "마이페이지 미리보기",
    "icons": {
      "128": "icon.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://playentry.org/project/*"],
        "js": ["script.js"],
        "css": ["script.css"],
        "all_frames": true
      }
    ]
  }